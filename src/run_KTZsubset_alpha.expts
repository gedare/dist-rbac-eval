#!/bin/bash
#./run_KTZsubset_alpha.expts

for X in 0.0 0.25 0.5 0.75 1.0
do
  x=${X}_1000checks
  echo "Doing alpha=${x}"
  cp -r dataset_KTZsubset_${x} dataset_KTZsubset
  python run_experiments.py -e KTZsubset_inter,KTZsubset_intra -a 1,3
  rm -r stats_KTZsubset_${x}
  mv stats_KTZsubset stats_KTZsubset_${x}
  rm -r dataset_KTZsubset
done
echo "all done"

exit 0
